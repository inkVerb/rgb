G=0
B=255
while [ $R -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((R++))
((CNT++))
done

# Magenta > White
R=255
G=0
B=255
while [ $G -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((G++))
((CNT++))
done

# White > 222222
A=255
while [ $A -gt 34 ]
do
./copyrgbtomov $A $A $A $CNT
((A--))
((CNT++))
done

# 222222 > Real Magenta
R=34
G=34
B=34
while [ $R -le 221 ]
do
./copyrgbtomov $R $G $B $CNT
((R++))
((B++))
((CNT++))
done

R=221
G=34
B=221
./copyrgbtomov $R $G $B $CNT
((CNT++))
./copyrgbtomov $R $G $B $CNT
((CNT++))
