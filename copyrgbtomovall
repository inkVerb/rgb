# Green UP
R=0
G=16
B=0
while [ $G -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((G++))
((CNT++))
done

# Green DN
R=0
G=255
B=0
while [ $G -gt 0 ]
do
./copyrgbtomov $R $G $B $CNT
((G--))
((CNT++))
done

# Red UP
R=0
G=0
B=0
while [ $R -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((R++))
((CNT++))
done

# Red DN
R=255
G=0
B=0
while [ $R -gt 0 ]
do
./copyrgbtomov $R $G $B $CNT
((R--))
((CNT++))
done

# Blue UP
R=0
G=0
B=0
while [ $B -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((B++))
((CNT++))
done

# Blue DN
R=0
G=0
B=255
while [ $B -gt 0 ]
do
./copyrgbtomov $R $G $B $CNT
((B--))
((CNT++))
done

# Black > White
A=0
while [ $A -le 255 ]
do
./copyrgbtomov $A $A $A $CNT
((A++))
((CNT++))
done


# White > Cyan
R=255
G=255
B=255
while [ $R -gt 0 ]
do
./copyrgbtomov $R $G $B $CNT
((R--))
((CNT++))
done

# Cyan > White
R=0
G=255
B=255
while [ $R -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((R++))
((CNT++))
done

# White > Yellow
R=255
G=255
B=255
while [ $B -gt 0 ]
do
./copyrgbtomov $R $G $B $CNT
((B--))
((CNT++))
done

# Cyan > White
R=255
G=255
B=0
while [ $B -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((B++))
((CNT++))
done

# White > Magenta
R=255
G=255
B=255
while [ $G -gt 0 ]
do
./copyrgbtomov $R $G $B $CNT
((G--))
((CNT++))
done

# Magenta > Red
R=255
G=0
B=255
while [ $B -gt 0 ]
do
./copyrgbtomov $R $G $B $CNT
((B--))
((CNT++))
done

# Red > Yellow
R=255
G=0
B=0
while [ $G -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((G++))
((CNT++))
done

# Yellow > Green
R=255
G=255
B=0
while [ $R -gt 0 ]
do
./copyrgbtomov $R $G $B $CNT
((R--))
((CNT++))
done

# Green > Cyan
R=0
G=255
B=0
while [ $B -le 255 ]
do
./copyrgbtomov $R $G $B $CNT
((B++))
((CNT++))
done

# Cyan > Blue
