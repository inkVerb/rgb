# RGB Solids

## R 0-255 ####
clc
clear all
close all
%%
Image=zeros(1080,1920,3);
Type1=uint8(Image);

for i=0:255
Type1(:,:,1)=i;
imwrite(Type1,['R' int2str(i) '-G0-B0.png'])
end

#####

## G 0-255 ####
clc
clear all
close all
%%
Image=zeros(1080,1920,3);
Type1=uint8(Image);

for i=0:255
Type1(:,:,2)=i;
imwrite(Type1,['R0-G' int2str(i) '-B0.png'])
end

#####

## B 0-255 ####
clc
clear all
close all
%%
Image=zeros(1080,1920,3);
Type1=uint8(Image);

for i=0:255
Type1(:,:,3)=i;
imwrite(Type1,['R0-G0-B' int2str(i) '.png'])
end

#####


# Absolute Spectrum

## R 255, G 0-255 ####
clc
clear all
close all
%%
Image=zeros(1080,1920,3);
Type1=uint8(Image);

for i=0:255
Type1(:,:,1)=255;
Type1(:,:,2)=i;
imwrite(Type1,['R255-G' int2str(i) '-B0.png'])
end

